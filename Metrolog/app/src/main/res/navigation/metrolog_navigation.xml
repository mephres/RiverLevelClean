<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/metrolog_navigation"
    app:startDestination="@id/navigation_events">

    <fragment
        android:id="@+id/navigation_events"
        android:name="com.intas.metrolog.ui.events.EventsFragment"
        android:label="EventsFragment"
        tools:layout="@layout/fragment_events"/>
    <fragment
        android:id="@+id/navigation_requests"
        android:name="com.intas.metrolog.ui.requests.RequestsFragment"
        android:label="RequestsFragment"
        tools:layout="@layout/fragment_requests"/>
    <fragment
        android:id="@+id/navigation_equip"
        android:name="com.intas.metrolog.ui.equip.EquipFragment"
        android:label="EquipFragment"
        tools:layout="@layout/fragment_equip"/>
    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.intas.metrolog.ui.chat.ChatFragment"
        android:label="ChatFragment"
        tools:layout="@layout/fragment_chat"/>
    <fragment
        android:id="@+id/navigation_more"
        android:name="com.intas.metrolog.ui.more.MoreFragment"
        android:label="MoreFragment"
        tools:layout="@layout/fragment_more"/>
</navigation>